{
  "name": "client",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "preDevBuild": "rimraf ./tsconfig.server.tsbuildinfo && tsc -p tsconfig.server.json",
    "dev": "npm run preDevBuild && concurrently --kill-others \"tsc -w -p tsconfig.server.json\" \"cross-env NODE_ENV=development EXTERNAL_SERVER_URL=http://localhost:3001 INTERNAL_SERVER_URL=http://localhost:3001 nodemon\"",
    "build:ssr": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "build": "npm run check-errors && npm run preDevBuild && vite build && npm rum build:ssr",
    "preview": "node server/index.js",
    "tsc": "tsc --noEmit --p ./tsconfig.json",
    "check-errors": "npm run tsc && npm run lint && npm run stylelint",
    "lint": "eslint \"**/*.{js,ts,tsx}\" --max-warnings=0",
    "stylelint": "stylelint \"src/**/*.{css,scss}\" --max-warnings=0",
    "lint:fix": "concurrently \"npm run lint -- --fix\" \"npm run stylelint -- --fix\"",
    "format": "prettier --write \"**/*.{js,ts,tsx,css,scss}\"",
    "prepare": "husky",
    "clear-vite-cache": "rimraf ./node_modules/.vite",
    "clear-eslint-cache": "rimraf ./node_modules/.cache/eslint",
    "clear-stylelint-cache": "rimraf ./node_modules/.cache/stylelint",
    "clear-cache": "concurrently --kill-others 'npm:clear-vite-cache' 'npm:clear-eslint-cache' 'npm:clear-stylelint-cache'"
  },
  "dependencies": {
    "@ant-design/icons": "^6.1.0",
    "@reduxjs/toolkit": "^2.10.1",
    "antd": "^5.27.4",
    "classnames": "^2.5.1",
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.0.2",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.9.6",
    "redux": "^5.0.1",
    "serialize-javascript": "^7.0.0",
    "styled-components": "^6.1.0"
  },
  "devDependencies": {
    "@testing-library/react": "^13.3.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/serialize-javascript": "^5.0.4",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^8.23.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "jest-environment-jsdom": "^29.0.1",
    "lefthook": "^1.3.9",
    "sass-embedded": "^1.93.2",
    "vite": "^7.1.11",
    "vite-plugin-checker": "^0.11.0",
    "vite-plugin-pwa": "^1.1.0",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "license": "MIT"
}
